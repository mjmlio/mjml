{
  "name": "mjml",
  "description": "MJML: the only framework that makes responsive-email easy",
  "version": "4.9.3",
  "main": "./lib/cjs/index.js",
  "module": "./lib/esm/index.js",
  "exports": {
    "imports": "./lib/esm/index.js",
    "require": "./lib/cjs/index.js"
  },
  "bin": {
    "mjml": "bin/mjml"
  },
  "files": [
    "bin",
    "lib"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mjmlio/mjml.git",
    "directory": "packages/mjml"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mjmlio/mjml/issues"
  },
  "homepage": "https://mjml.io",
  "scripts": {
    "clean": "rimraf lib",
    "build": "babel src --out-dir lib/cjs --root-mode upward",
    "build:esm": "babel src --out-dir lib/esm --root-mode upward",
    "test": "node ./test/test-html-attributes.js"
  },
  "dependencies": {
    "@babel/runtime": "^7.8.7",
    "mjml-cli": "4.9.3",
    "mjml-core": "4.9.3",
    "mjml-migrate": "4.9.3",
    "mjml-preset-core": "4.9.3",
    "mjml-validator": "4.9.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "chai": "^4.1.1",
    "cheerio": "1.0.0-rc.10",
    "lodash": "^4.17.15",
    "rimraf": "^3.0.2"
  }
}